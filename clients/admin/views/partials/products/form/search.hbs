<el-form :label-position="labelPositionTop" :model="searchForm" :rules="searchValidate" ref="searchForm"
    label-width="120px" class="demo-ruleForm" size="mini">
    <div class="row">
        <div class="col-lg-6">
            <el-form-item label="Thời gian bắt đầu">
                <el-date-picker class="w-100" v-model="searchForm.dateStart" type="datetime"
                    placeholder="Vui lòng chọn thời gian bắt đầu">
                </el-date-picker>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Thời gian kết thúc">
                <el-date-picker class="w-100" v-model="searchForm.dateEnd" type="datetime"
                    placeholder="Vui lòng chọn thời gian kết thúc">
                </el-date-picker>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Số tiền bắt đầu">
                <el-input class="w-100" v-model="searchForm.priceStart" type="datetime"
                    placeholder="Vui lòng chọn số tiền bắt đầu">
                </el-input>
            </el-form-item>
        </div>
        <div class="col-lg-6">
            <el-form-item label="Số tiền kết thúc">
                <el-input class="w-100" v-model="searchForm.priceEnd" type="datetime"
                    placeholder="Vui lòng chọn số tiền kết thúc">
                </el-input>
            </el-form-item>
        </div>
        <div class="col-lg-12">
            <el-button @click="searchByProduct('searchForm')" size="mini" class="float-right" type="danger">Tìm kiếm
            </el-button>
        </div>
    </div>
</el-form>